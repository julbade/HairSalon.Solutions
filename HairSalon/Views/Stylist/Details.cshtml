@{
Layout = "_Layout";
}
<h3>Here are all the clients for you:</h3>
<ol>
@foreach (var item in @Model["client"])
{
  <li><a href='/stylists/@Model["stylist"].GetId()/clients/@item.GetId()'>@item.GetName()</a></li>
}
</ol>

<br>

<a href='/stylists/new'>Add another stylist</a>
<br>
<a href='/stylists'>View all Stylists</a>
<br>
<p><a href='/stylists/@Model["stylist"].GetId()/clients/new'>Add a client</a></p>
